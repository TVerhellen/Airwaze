@model List<UserListViewModel>

<div>
    <table>
        <tr>
            <th>Firstname</th>
            <th>Lastname</th>
        </tr>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName </td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td><a asp-controller="User" asp-action="Detail" asp-route-UserID=@user.UserID>Detail</a></td>
            </tr>
        }

    </table>
</div>